{
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
  },
  "hosting": {
    "site": "shadyantra",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/node_modules/**",
      "**/.*"
    ],
    "redirects": [
      {
        "source": "/about",
        "destination": "/",
        "type": 301
      }
    ]
  }
}